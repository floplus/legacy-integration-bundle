<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="webfactory.kernel.integration.class">Webfactory\Bundle\KernelIntegrationBundle\KernelIntegration</parameter>
    </parameters>

    <services>
        <service id="webfactory.kernel.integration" class="%webfactory.kernel.integration.class%">
            <argument type="service" id="http_kernel" />
        </service>
    </services>
</container>
